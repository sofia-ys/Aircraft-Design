import math
S = 173.77  # wing area m^2
AR = 8.3  # aspect ratio
b = math.sqrt(AR*S)  # wing span m
taper = 0.292  # taper ratio
cRoot = (2*S)/((taper+1)*b) # root chord m
cTip = taper*cRoot  # tip chord m
sweep = 0.537  # wing sweep rad

